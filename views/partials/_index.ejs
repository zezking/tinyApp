<div class="main-content">
    <main style="margin: 15em">
      <h3>My URLs</h3>
   
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Short URL</th>
            <th scope="col">Long URL</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
          </tr>
        </thead>
        <tbody>
          <% for(let url in urls) { %>
          <tr>
            <td><a href="/urls/<%= url %>"><%= url %></a></td>
            <td><%= urls[url] %></td>
            <td>
              <form method="POST" action="/urls/<%= url %>/edit"">
                <button type="edit" class="btn btn-outline-primary btn-sm">
                  edit
                </button>
              </form>
            </td>
            <td>
              <form method="POST" action="/urls/<%= url %>/delete"">
                <button type="delete" class="btn btn-outline-danger btn-sm">
                  delete
                </button>
              </form>
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </main>
  </div>